{
	"compilerOptions": {
		"outDir": "./build/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"moduleResolution": "node",
		"baseUrl": "src",
		"paths": {
			"api": ["api/*"],
			"components": ["components/*"],
			"store":["store/*"],
			"utils": ["utils/*"],
		}
	}
}